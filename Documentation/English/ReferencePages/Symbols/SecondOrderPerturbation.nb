Cell["\<\
Given a matrix, its eigenvectors and eigenvalues are sufficient to determine \
how the eigenvalues change to second order given a perturbation added to it .\
\
\>", "ExampleText",
 CellChangeTimes->{{3.946566596844522*^9, 3.9465666317257423`*^9}, 
   3.946566669736165*^9, {3.9465672249923353`*^9, 3.946567232808704*^9}},
 CellID->250434193,ExpressionUUID->"5659e641-8173-4fd5-a2c7-860d9cfb3189"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"A", "=", 
   RowBox[{"N", "@", 
    RowBox[{"RandomInteger", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"-", "10"}], ",", "10"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"10", ",", "10"}], "}"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"A", "=", 
   RowBox[{"A", "+", 
    RowBox[{"Transpose", "[", "A", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"P", "=", 
   RowBox[{"N", "@", 
    RowBox[{"RandomInteger", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"-", "10"}], ",", "10"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"10", ",", "10"}], "}"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"P", " ", "=", " ", 
   RowBox[{"P", "+", 
    RowBox[{"Transpose", "[", "P", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"eigenValues", ",", "eigenVectors"}], "}"}], "=", 
   RowBox[{"Eigensystem", "[", "A", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"sorter", "=", 
   RowBox[{"Ordering", "[", "eigenValues", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"eigenValues", "=", 
   RowBox[{"eigenValues", "[", 
    RowBox[{"[", "sorter", "]"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"eigenVectors", "=", 
   RowBox[{"eigenVectors", "[", 
    RowBox[{"[", "sorter", "]"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"gradients", "=", 
   RowBox[{"FirstOrderPerturbation", "[", 
    RowBox[{"eigenVectors", ",", "P"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"secondDerivatives", "=", 
   RowBox[{"SecondOrderPerturbation", "[", 
    RowBox[{"eigenValues", ",", "eigenVectors", ",", "P"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Kappa]max", "=", "0.25"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"idx", "=", "1"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"direct", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"\[Kappa]", ",", 
       RowBox[{
        RowBox[{"Sort", "[", 
         RowBox[{"Eigenvalues", "[", 
          RowBox[{"A", "+", 
           RowBox[{"\[Kappa]", " ", "P"}]}], "]"}], "]"}], "[", 
        RowBox[{"[", "idx", "]"}], "]"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Kappa]", ",", "0", ",", "\[Kappa]max", ",", 
       RowBox[{"\[Kappa]max", "/", "10"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"perturbedonce", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"\[Kappa]", ",", 
       RowBox[{
        RowBox[{"eigenValues", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "+", 
        RowBox[{
         RowBox[{"gradients", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "*", "\[Kappa]"}]}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Kappa]", ",", "0", ",", "\[Kappa]max", ",", 
       RowBox[{"\[Kappa]max", "/", "10"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"perturbedtwice", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"\[Kappa]", ",", 
       RowBox[{
        RowBox[{"eigenValues", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "+", 
        RowBox[{
         RowBox[{"gradients", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "*", "\[Kappa]"}], "+", 
        RowBox[{
         FractionBox["1", "2"], 
         SuperscriptBox["\[Kappa]", "2"], 
         RowBox[{"secondDerivatives", "[", 
          RowBox[{"[", "1", "]"}], "]"}]}]}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Kappa]", ",", "0", ",", "\[Kappa]max", ",", 
       RowBox[{"\[Kappa]max", "/", "10"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"ListPlot", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"direct", ",", "perturbedonce", ",", "perturbedtwice"}], "}"}], 
   ",", 
   RowBox[{"Frame", "->", "True"}], ",", 
   RowBox[{"FrameLabel", "->", 
    RowBox[{"{", 
     RowBox[{"\"\<\[Kappa]\>\"", ",", "\"\<\[Lambda]\>\""}], "}"}]}], ",", 
   RowBox[{"PlotLegends", "->", 
    RowBox[{"{", 
     RowBox[{
     "\"\<Exact\>\"", ",", "\"\<1st Order\>\"", ",", "\"\<2n Order\>\""}], 
     "}"}]}]}], "]"}]}], "Input",
 CellChangeTimes->{{3.946566086617399*^9, 3.9465662537471457`*^9}, {
