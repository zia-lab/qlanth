Cell[CellGroupData[{
Cell["SecondOrderPerturbation", "ObjectName",
 CellID->1106231919,ExpressionUUID->"7405ada2-e256-45ab-875c-27b57491dc29"],

Cell[TextData[{
 Cell["   ", "ModInfo",ExpressionUUID->"5cd2e90e-20f8-483e-b50d-d6693db5e37b"],
 Cell[BoxData[
  RowBox[{
   ButtonBox["SecondOrderPerturbation",
    BaseStyle->"Link",
    ButtonData->"paclet:qlanth/ref/SecondOrderPerturbation"], 
   "[", 
   StyleBox[
    RowBox[{"eigenValues", ",", "eigenVectors", ",", "perturbationMatrix"}],
    FontSlant->"Italic"], "]"}]], "InlineFormula",ExpressionUUID->
  "ef78231f-4ddd-4ac5-b446-e798a408a0a6"],
 " \[LineSeparator]given the ",
 StyleBox["eigenValues",
  FontSlant->"Italic"],
 " and ",
 StyleBox["eigenVectors",
  FontSlant->"Italic"],
 " of a matrix A (which doesn't need to be given) together with a \
corresponding perturbation matrix ",
 StyleBox["perturbationMatrix",
  FontSlant->"Italic"],
 ", this function calculates the second derivative of the eigenvalues with \
respect to the scale factor of the perturbation matrix. In the sense that the \
eigenvalues of the matrix A + \[Beta] perturbationMatrix are to second order \
equal to ",
 Cell[BoxData[
  SubscriptBox["\[Lambda]", "i"]], "InlineFormula",ExpressionUUID->
  "8b9b49e5-586a-4aea-806d-31777f9b0390"],
 " + ",
 Cell[BoxData[
  SubscriptBox["\[Delta]", "i"]], "InlineFormula",ExpressionUUID->
  "fc2afd3d-da22-4ebb-af10-31484e2aac52"],
 " \[Beta] + ",
 Cell[BoxData[
  RowBox[{
   FractionBox["1", "2"], "\[Zeta]i"}]], "InlineFormula",ExpressionUUID->
  "84794eb7-90f9-4ed8-b2ea-f4c70335126a"],
 " ",
 Cell[BoxData[
  SuperscriptBox["\[Beta]", "2"]], "InlineFormula",ExpressionUUID->
  "f1974540-705b-4b81-b8d7-88d66934a870"],
 ", where the ",
 Cell[BoxData[
 "\[Zeta]i"], "InlineFormula",ExpressionUUID->
  "30796f0a-e4af-4166-86b3-e78d6ab4ffe6"],
 " are the values returned by this function. "
}], "Usage",
 CellChangeTimes->{{3.946566929469227*^9, 3.946567017687999*^9}, {
  3.946567347239841*^9, 3.946567415300324*^9}, {3.94664123036371*^9, 
  3.9466412308788843`*^9}},
 CellID->1673930554,ExpressionUUID->"20009500-5b82-446a-b84b-db6885dbe788"],

Cell["\<\
The eigenvalues and eigenvectors are assumed to be given in the same order, \
i.e. the i-th eigenvalue corresponds to the i-th eigenvector. \
\>", "Notes",
 CellChangeTimes->{{3.9465674166336308`*^9, 3.946567430187392*^9}},
 CellID->1715045378,ExpressionUUID->"6496dc25-bfb2-43e3-ab87-46c4d168fdbd"],

Cell["Function assumes that the eigenvalues are non-degenerate.", "Notes",
 CellChangeTimes->{{3.946567421527565*^9, 3.946567425141284*^9}},
 CellID->1681909134,ExpressionUUID->"aa714aa2-adc7-4adf-8678-91d43bffd0cf"]
}, Open  ]],

