                    RowBox[{"If", "[", 
                    RowBox[{
                    RowBox[{"!", 
                    RowBox[{"OptionValue", "[", "\"Sparse\"", "]"}]}], ",", 
                    RowBox[{"numHam", "=", 
                    RowBox[{"Normal", "[", "numHam", "]"}]}]}], "]"}], ";", 
                    
                    RowBox[{"PrintFun", "[", 
                    "\"> Calculating the SLJ basis ...\"", "]"}], ";", 
                    RowBox[{"basis", "=", 
                    RowBox[{"BasisLSJMJ", "[", "numE", "]"}]}], ";", 
                    RowBox[{"If", "[", 
                    RowBox[{"onlyEnergies", ",", 
                    RowBox[{
                    
                    RowBox[{"PrintFun", "[", 
