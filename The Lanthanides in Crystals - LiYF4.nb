                    "exportFname", ",", "tinyexportFname", ",", "tinyExport", 
                    ",", "nbFname", ",", "uncertaintySentence", ",", 
                    "sigmaRMS", ",", "energyUncertaintyTemplate"}], "}"}], 
                    ",", 
                    RowBox[{
                    RowBox[{"paramKeys", "=", 
                    RowBox[{"Keys", "[", "params", "]"}]}], ";", 
                    RowBox[{"Which", "[", 
                    RowBox[{
                    RowBox[{"MemberQ", "[", 
                    RowBox[{"paramKeys", ",", "nE"}], "]"}], ",", 
                    RowBox[{"numE", "=", 
                    RowBox[{"params", "[", "nE", "]"}]}], ",", 
                    RowBox[{"MemberQ", "[", 
                    RowBox[{"paramKeys", ",", "\"nE\""}], "]"}], ",", 
                    RowBox[{"numE", "=", 
                    RowBox[{"params", "[", "\"nE\"", "]"}]}], ",", 
                    RowBox[{"MemberQ", "[", 
                    RowBox[{"paramKeys", ",", "\"numE\""}], "]"}], ",", 
                    RowBox[{"numE", "=", 
                    RowBox[{"params", "[", "\"numE\"", "]"}]}], ",", "True", 
                    ",", 
                    RowBox[{
                    
                    RowBox[{"Print", "[", 
                    "\"The number of electrons was not specified in params, \
exiting ...\"", "]"}], ";", 
                    RowBox[{"Return", "[", "Null", "]"}], ";"}]}], "]"}], ";", 
