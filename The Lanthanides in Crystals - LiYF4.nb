                    RowBox[{"sigmaRMS", "=", 
                    
                    RowBox[{"OptionValue", "[", "\"Energy Uncertainty in K\"",
                     "]"}]}], ";", 
                    RowBox[{"uncertaintySentence", "=", 
                    RowBox[{"If", "[", 
                    RowBox[{
