                    ",", "maxStatesInTable", ",", "Duplicator", ",", "host", 
                    ",", "ln", ",", "terms", ",", "termNames", ",", 
                    "termSimplifier", ",", "startTime", ",", "numH", ",", 
                    "numEH", ",", "simpleHam", ",", "endTime", ",", 
                    "loadTime", ",", "numHam", ",", "basis", ",", "eigensys", 
                    ",", "eigenEnergies", ",", "titleTemplate", ",", "title", 
                    ",", "parsedStates", ",", "stateLabels", ",", 
